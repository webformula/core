{ "types": {
    "itemPlural": {
      "type": "cardinal",
      "zero": "items",
      "one": "item",
      "two": "items",
      "few": "items",
      "many": "items",
      "other": "items" },
    "placeOrdinal": {
      "type": "ordinal",
      "one": "st",
      "two": "nd",
      "few": "rd",
      "other": "th" },
    "minutesCardinal": {
      "type": "cardinal",
      "one": "minute",
      "other": "minutes" },
    "dateFormat": {
      "type": "date",
      "options": {
        "dateStyle": "short",
        "timeStyle": "short" } },
    "currencyFormat": {
      "type": "number",
      "options": {
        "style": "currency",
        "currency": "USD" } } },

  "About": "About",
  "Label": "Label",
  "key_string": "This is a key string",
  "item_count": "${count} ${itemPlural(count)}",
  "place_ordinal": "${place} ${placeOrdinal(place)}",
  "time_from_now": "${time} ${minutesCardinal(minutes)} from now",
  "date_key": "${dateFormat(date)}",
  "currency_key": "${currencyFormat(currency)}" }
